{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "coord",
    "weather",
    "base",
    "main",
    "visibility",
    "wind",
    "clouds",
    "dt",
    "sys",
    "id",
    "name",
    "cod"
  ],
  "properties": {
    "coord": {
      "$id": "#/properties/coord",
      "type": "object",
      "title": "The Coord Schema",
      "required": [
        "lon",
        "lat"
      ],
      "properties": {
        "lon": {
          "$id": "#/properties/coord/properties/lon",
          "type": "number",
          "title": "The Lon Schema",
          "default": 0.0,
          "examples": [
            -122.42
          ]
        },
        "lat": {
          "$id": "#/properties/coord/properties/lat",
          "type": "number",
          "title": "The Lat Schema",
          "default": 0.0,
          "examples": [
            37.78
          ]
        }
      }
    },
    "weather": {
      "$id": "#/properties/weather",
      "type": "array",
      "title": "The Weather Schema",
      "items": {
        "$id": "#/properties/weather/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "main",
          "description",
          "icon"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/weather/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              803
            ]
          },
          "main": {
            "$id": "#/properties/weather/items/properties/main",
            "type": "string",
            "title": "The Main Schema",
            "default": "",
            "examples": [
              "Clouds"
            ],
            "pattern": "^(.*)$"
          },
          "description": {
            "$id": "#/properties/weather/items/properties/description",
            "type": "string",
            "title": "The Description Schema",
            "default": "",
            "examples": [
              "broken clouds"
            ],
            "pattern": "^(.*)$"
          },
          "icon": {
            "$id": "#/properties/weather/items/properties/icon",
            "type": "string",
            "title": "The Icon Schema",
            "default": "",
            "examples": [
              "04d"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "base": {
      "$id": "#/properties/base",
      "type": "string",
      "title": "The Base Schema",
      "default": "",
      "examples": [
        "stations"
      ],
      "pattern": "^(.*)$"
    },
    "main": {
      "$id": "#/properties/main",
      "type": "object",
      "title": "The Main Schema",
      "required": [
        "temp",
        "pressure",
        "humidity",
        "temp_min",
        "temp_max"
      ],
      "properties": {
        "temp": {
          "$id": "#/properties/main/properties/temp",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Temp Schema",
          "default": 0.0,
          "examples": [
            294.68
          ]
        },
        "pressure": {
          "$id": "#/properties/main/properties/pressure",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Pressure Schema",
          "default": 0,
          "examples": [
            1013
          ]
        },
        "humidity": {
          "$id": "#/properties/main/properties/humidity",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Humidity Schema",
          "default": 0,
          "examples": [
            62
          ]
        },
        "temp_min": {
          "$id": "#/properties/main/properties/temp_min",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Temp_min Schema",
          "default": 0.0,
          "examples": [
            292.05
          ]
        },
        "temp_max": {
          "$id": "#/properties/main/properties/temp_max",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Temp_max Schema",
          "default": 0.0,
          "examples": [
            297.05
          ]
        }
      }
    },
    "visibility": {
      "$id": "#/properties/visibility",
      "type": "integer",
      "title": "The Visibility Schema",
      "default": 0,
      "examples": [
        16093
      ]
    },
    "wind": {
      "$id": "#/properties/wind",
      "type": "object",
      "title": "The Wind Schema",
      "required": [
        "speed",
        "deg"
      ],
      "properties": {
        "speed": {
          "$id": "#/properties/wind/properties/speed",
          "type": [
            "number",
            "integer"
          ],
          "title": "The Speed Schema",
          "default": 0.0,
          "examples": [
            3.1
          ]
        },
        "deg": {
          "$id": "#/properties/wind/properties/deg",
          "type": "integer",
          "title": "The Deg Schema",
          "default": 0,
          "examples": [
            30
          ]
        }
      }
    },
    "clouds": {
      "$id": "#/properties/clouds",
      "type": "object",
      "title": "The Clouds Schema",
      "required": [
        "all"
      ],
      "properties": {
        "all": {
          "$id": "#/properties/clouds/properties/all",
          "type": "integer",
          "title": "The All Schema",
          "default": 0,
          "examples": [
            75
          ]
        }
      }
    },
    "dt": {
      "$id": "#/properties/dt",
      "type": "integer",
      "title": "The Dt Schema",
      "default": 0,
      "examples": [
        1538333880
      ]
    },
    "sys": {
      "$id": "#/properties/sys",
      "type": "object",
      "title": "The Sys Schema",
      "required": [
        "type",
        "id",
        "message",
        "country",
        "sunrise",
        "sunset"
      ],
      "properties": {
        "type": {
          "$id": "#/properties/sys/properties/type",
          "type": "integer",
          "title": "The Type Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "id": {
          "$id": "#/properties/sys/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            478
          ]
        },
        "message": {
          "$id": "#/properties/sys/properties/message",
          "type": "number",
          "title": "The Message Schema",
          "default": 0.0,
          "examples": [
            0.0043
          ]
        },
        "country": {
          "$id": "#/properties/sys/properties/country",
          "type": "string",
          "title": "The Country Schema",
          "default": "",
          "examples": [
            "US"
          ],
          "pattern": "^(.*)$"
        },
        "sunrise": {
          "$id": "#/properties/sys/properties/sunrise",
          "type": "integer",
          "title": "The Sunrise Schema",
          "default": 0,
          "examples": [
            1538316313
          ]
        },
        "sunset": {
          "$id": "#/properties/sys/properties/sunset",
          "type": "integer",
          "title": "The Sunset Schema",
          "default": 0,
          "examples": [
            1538358769
          ]
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        5391959
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "San Francisco"
      ],
      "pattern": "^(.*)$"
    },
    "cod": {
      "$id": "#/properties/cod",
      "type": "integer",
      "title": "The Cod Schema",
      "default": 0,
      "examples": [
        200
      ]
    }
  }
}
