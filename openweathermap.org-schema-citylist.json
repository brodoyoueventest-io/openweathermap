{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "OpenWeatherMap - List of Cities",
  "required": [
    "cod",
    "calctime",
    "cnt",
    "list"
  ],
  "properties": {
    "cod": {
      "$id": "#/properties/cod",
      "type": "integer",
      "title": "The Cod Schema",
      "default": 0,
      "examples": [
        200
      ]
    },
    "calctime": {
      "$id": "#/properties/calctime",
      "type": "number",
      "title": "The Calctime Schema",
      "default": 0.0,
      "examples": [
        0.236668675
      ]
    },
    "cnt": {
      "$id": "#/properties/cnt",
      "type": "integer",
      "title": "The Cnt Schema",
      "default": 0,
      "examples": [
        14
      ]
    },
    "list": {
      "$id": "#/properties/list",
      "type": "array",
      "title": "The List Schema",
      "items": {
        "$id": "#/properties/list/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "dt",
          "name",
          "coord",
          "main",
          "wind",
          "rain",
          "snow",
          "clouds",
          "weather"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/list/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              7262761
            ]
          },
          "dt": {
            "$id": "#/properties/list/items/properties/dt",
            "type": "integer",
            "title": "The Dt Schema",
            "default": 0,
            "examples": [
              1538337728
            ]
          },
          "name": {
            "$id": "#/properties/list/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Makakilo"
            ],
            "pattern": "^(.*)$"
          },
          "coord": {
            "$id": "#/properties/list/items/properties/coord",
            "type": "object",
            "title": "The Coord Schema",
            "required": [
              "Lat",
              "Lon"
            ],
            "properties": {
              "Lat": {
                "$id": "#/properties/list/items/properties/coord/properties/Lat",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Lat Schema",
                "default": 0.0,
                "examples": [
                  21.352369
                ]
              },
              "Lon": {
                "$id": "#/properties/list/items/properties/coord/properties/Lon",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Lon Schema",
                "default": 0.0,
                "examples": [
                  -158.086548
                ]
              }
            }
          },
          "main": {
            "$id": "#/properties/list/items/properties/main",
            "type": "object",
            "title": "The Main Schema",
            "required": [
              "temp",
              "temp_min",
              "temp_max",
              "pressure",
              "humidity"
            ],
            "properties": {
              "temp": {
                "$id": "#/properties/list/items/properties/main/properties/temp",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Temp Schema",
                "default": 0.0,
                "examples": [
                  28.18
                ]
              },
              "temp_min": {
                "$id": "#/properties/list/items/properties/main/properties/temp_min",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Temp_min Schema",
                "default": 0.0,
                "examples": [
                  26.6
                ]
              },
              "temp_max": {
                "$id": "#/properties/list/items/properties/main/properties/temp_max",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Temp_max Schema",
                "default": 0.0,
                "examples": [
                  28.9
                ]
              },
              "pressure": {
                "$id": "#/properties/list/items/properties/main/properties/pressure",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Pressure Schema",
                "default": 0,
                "examples": [
                  1016
                ]
              },
              "humidity": {
                "$id": "#/properties/list/items/properties/main/properties/humidity",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Humidity Schema",
                "default": 0,
                "examples": [
                  65
                ]
              }
            }
          },
          "wind": {
            "$id": "#/properties/list/items/properties/wind",
            "type": "object",
            "title": "The Wind Schema",
            "required": [
              "speed",
              "deg"
            ],
            "properties": {
              "speed": {
                "$id": "#/properties/list/items/properties/wind/properties/speed",
                "type": [
                  "number",
                  "integer"
                ],
                "title": "The Speed Schema",
                "default": 0.0,
                "examples": [
                  3.1
                ]
              },
              "deg": {
                "$id": "#/properties/list/items/properties/wind/properties/deg",
                "type": "integer",
                "title": "The Deg Schema",
                "default": 0,
                "examples": [
                  0
                ]
              }
            }
          },
          "rain": {
            "$id": "#/properties/list/items/properties/rain",
            "type": "null",
            "title": "The Rain Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "snow": {
            "$id": "#/properties/list/items/properties/snow",
            "type": "null",
            "title": "The Snow Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "clouds": {
            "$id": "#/properties/list/items/properties/clouds",
            "type": "object",
            "title": "The Clouds Schema",
            "required": [
              "today"
            ],
            "properties": {
              "today": {
                "$id": "#/properties/list/items/properties/clouds/properties/today",
                "type": "integer",
                "title": "The Today Schema",
                "default": 0,
                "examples": [
                  1
                ]
              }
            }
          },
          "weather": {
            "$id": "#/properties/list/items/properties/weather",
            "type": "array",
            "title": "The Weather Schema",
            "items": {
              "$id": "#/properties/list/items/properties/weather/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "id",
                "main",
                "description",
                "icon"
              ],
              "properties": {
                "id": {
                  "$id": "#/properties/list/items/properties/weather/items/properties/id",
                  "type": "integer",
                  "title": "The Id Schema",
                  "default": 0,
                  "examples": [
                    521
                  ]
                },
                "main": {
                  "$id": "#/properties/list/items/properties/weather/items/properties/main",
                  "type": "string",
                  "title": "The Main Schema",
                  "default": "",
                  "examples": [
                    "Rain"
                  ],
                  "pattern": "^(.*)$"
                },
                "description": {
                  "$id": "#/properties/list/items/properties/weather/items/properties/description",
                  "type": "string",
                  "title": "The Description Schema",
                  "default": "",
                  "examples": [
                    "shower rain"
                  ],
                  "pattern": "^(.*)$"
                },
                "icon": {
                  "$id": "#/properties/list/items/properties/weather/items/properties/icon",
                  "type": "string",
                  "title": "The Icon Schema",
                  "default": "",
                  "examples": [
                    "09d"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }
}
